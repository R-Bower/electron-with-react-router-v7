{
  "name": "electron-with-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "out/main/index.mjs",
  "scripts": {
    "dev": "pnpm build:preload && DEV_SERVER=1 pnpm dev:electron",
    "dev:renderer": "remix vite:dev ./src/renderer/",
    "dev:electron": "pnpm build:main && electron .",
    "build:main": "vite build --config ./src/main/vite.config.ts",
    "build:preload": "vite build --config ./src/preload/vite.config.ts",
    "build:renderer": "vite build ./src/renderer",
    "build:unpack": "pnpm run build:main && pnpm run build:preload && pnpm run build:renderer && electron-builder --dir"
  },
  "dependencies": {
    "@remix-run/node": "^2.14.0",
    "electron-serve": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.14.0",
    "@remix-run/react": "^2.14.0",
    "@remix-run/serve": "^2.14.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-node-polyfills": "^0.22.0"
  },
  "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b",
  "exports": {
    ".": {
      "import": "./out/main/index.mjs"
    }
  },
  "author": "tmtk75"
}
